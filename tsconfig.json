
{
  "compileOnSave": false,
  "compilerOptions": {
    "esModuleInterop": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./src",
    "jsx": "preserve",
    "preserveConstEnums": true,
    "module": "es2015",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "outDir": "./dist/build/",
    "paths": {
      "FunBlocks/*": ["./*"]
    },
    "removeComments": false,
    "skipLibCheck": true,
    "sourceMap": true,
    "target": "es2016",
    "types": ["webpack-env", "jest"]
  }
}
